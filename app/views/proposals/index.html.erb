  <section class="proposals">
    <header class="proposal-head">
      <h2><%= proposal_info(:heading) %></h2>
      <p><%= proposal_info(:description) %></p>
    </header>
    <%= render partial: 'proposal', :collection => @proposals %>
  </section>
</section>